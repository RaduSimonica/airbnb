<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Main Test Suite" verbose="1" parallel="methods" thread-count="20">
    <listeners>
        <listener class-name="reporting.listeners.SuiteInterceptor"/>
        <listener class-name="reporting.listeners.TestInterceptor"/>
    </listeners>
    <test name="All Tests">
        <packages>
            <package name="tests"/>
        </packages>
    </test>
</suite>